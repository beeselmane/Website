ErrorDocument 401 /index.php
ErrorDocument 403 /index.php
ErrorDocument 404 /index.php
ErrorDocument 500 /error.php
ErrorDocument 503 /error.php
RewriteCond %{HTTP_HOST} ^ww\.beeselmane\.com$ [OR]
RewriteCond %{HTTP_HOST} ^www\.ww\.beeselmane\.com$
RewriteRule ^/?$ "http\:\/\/www\.beeselmane\.com" [R=301,L]

allow from all
